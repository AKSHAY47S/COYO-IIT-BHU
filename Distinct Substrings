#include <bits/stdc++.h>
using namespace std;

int main() {
	
	int t; cin>>t;
	set<string>st;
	
	while(t--)
	{
	    string s; cin>>s;
	    int n=s.length();
	    
	    
	    for(int i=0;i<n-2+1;i++)
	      st.insert(s.substr(i,2));
	      
	    cout<<st.size()<<endl;
	    st.clear();
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	return 0;
}
