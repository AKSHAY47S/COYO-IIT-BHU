#include <bits/stdc++.h>
using namespace std;

int main() {
	
string s; getline(cin,s);

  for(int i=0;i<s.length();i++)
  {
      if(('a'<=s[i] && 'z'>=s[i]) || ('A'<=s[i] && 'Z'>=s[i]) || (s[i]=' '))
        continue;
      else
        s.erase(i,1);
          
  }
  
  cout<<s<<endl;
	
	
	return 0;
}
