#include<bits/stdc++.h>
using namespace std;

int distance_matrix[100][100];

void floyd_warshell()
{
    for(int k=1;k<=v;k++)
    {
        for(int i=1;i<=v;i++)
        {
            for(int j=1;j<=v;j++)
            {
                if(distance[i][j]>distance[i][k]+distance[k][j])
                  distance[i][j]=distance[i][k]+distance[k][j];
            }
        }
    }
}

